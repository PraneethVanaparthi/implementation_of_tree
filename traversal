#include<stdio.h>
#include<stdlib.h>
struct node{
int item;
struct node*left;
struct node*right;
};
void inorder(struct node*root){
if(root==null)
return;
inorder(root->left);
printf("%d",root->item);
inorder(root->right);
}
void preorder(struct node*root){
if(root==null){
return;
preorder(root->left);
preorder(root->right)
}
//post order
void postorder(struct node*root){
if(root==Null)
return ;
postorder(root->left);
postorder(root->right);
